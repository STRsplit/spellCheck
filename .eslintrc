{
  "extends": "airbnb",
  "rules": {
    "arrow-parens": "off",
    "func-names": ["error", "never"],
    "quotes": 0,
    "indent": [2, 2],
    "keyword-spacing": "off",
    "space-after-keywords": 0,
    "space-before-blocks": [0, "always"],
    "space-before-function-paren": [0, {"anonymous": "always", "named": "never"}],
    "no-plusplus": "off"
  }
}